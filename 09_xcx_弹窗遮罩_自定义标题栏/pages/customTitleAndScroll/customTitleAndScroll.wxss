}

.title_bar {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  z-index: 996;
  background: linear-gradient(270deg, #EE0407, #FD1B49 100%);
}

/* 自定义标题栏end */
/* 切换 */
/* 全部 已付款 */
.nav-container {
  position: relative;
  z-index: 100;
  width: 100vw;
  height: 70rpx;
  display: flex;
  align-items: center;
  background: linear-gradient(270deg, #EE0407, #FD1B49 100%);
}

.nav-container .nav-item {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.nav-item text {
  display: block;
  color: #fff;
  font-size: 26rpx;
  font-weight: 400;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 6rpx solid transparent;
}

/* nav 选中状态 */

.nav-item.on text {
  height: 86rpx;
  line-height: 86rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
}

.nav-bottom {
  width: 40rpx;
  height: 6rpx;
  background: #fff;
  border-radius: 4rpx;
  position: absolute;
  bottom: 0;
}

.bbctouch-order-item-white {
  width: 100%;
  height: 19rpx;
  background-color: #fff;
}
/* banner start */
.jm_banner{
  margin-top: 40rpx;
  width: 750rpx;
  position: relative;
  z-index: 60;
  display: flex;
  justify-content: center;
}
.swiper-container{
  width: 690rpx;
